        "feed": "^4.2.2",
        "gray-matter": "^4.0.3",
        "motion": "^12.6.3",
        "next": "15.2.1",
        "next-mdx-remote": "^5.0.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-icons": "^5.5.0",
        "reading-duration": "^1.1.2",
